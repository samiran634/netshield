{
    "manifest_version": 3,
    "name": "netshild",
    "version": "1.0.0",
    "action": { 
      "default_popup": "index.html",
      "default_icon": {
        "16": "images/logo.png",
        "48": "images/logo.png",
        "128": "images/logo.png"
      }
  
  },
    "content_scripts": [
      {
        "matches": ["https://*.youtube.com/*"],
        "js": ["contentScript.js"]
      }
    ],
    "host_permissions": ["https://*.youtube.com/*"],
    "permissions": 
        [
            "storage"
            ,"scripting"
        ]
    ,
    "background": {
      "service_worker": "service-worker-loader.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["icons/*"],
        "matches": ["https://*.youtube.com/*"]
      }
    ]
  }